{
  "expo": {
    "name": "Draw With Me",
    "slug": "capstone-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/signinLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/signinLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hoangloc1403.capstonemobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/signinLogo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "58286878-caf2-4bc0-aa2a-3bd9f78b7202"
      }
    }
  }
}